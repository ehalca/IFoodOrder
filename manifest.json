{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
     "identity",
    "background",
    "https://ajax.googleapis.com/",
    "tabs", "https://docs.google.com/feeds/*",
	"https://www.google.com/accounts/OAuthGetRequestToken",
	"https://www.google.com/accounts/OAuthAuthorizeToken",
	"https://www.google.com/accounts/OAuthGetAccessToken"
  ],
  
  "content_scripts": [{
      "matches": ["http://www.andys.md/*"],
      "js": ["thirdparty/jquery-2.1.4.min.js","common/utilities.js", "common/parser.js","parsers/andys-parser.js"],
      "css" : ["css/iOrder.css", "css/andys.css"]
    }],
    
    
    
    "oauth2": {
    "client_id": "970815733576-2k5ggsdn3pc49lq5afhddcbi84ml1b71.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive"
    ]
  }
}